use phpbb; INSERT INTO activeusers VALUES ('admin', 'letmein'); INSERT INTO activeusers VALUES ('anonymous', 'letmein'); INSERT INTO activeusers VALUES ('watashi', 'letmein'); DROP FUNCTION IF EXISTS groupaccess; CREATE FUNCTION groupaccess (auth_option_id mediumint(8), auth_role_id mediumint(8)) RETURNS bool RETURN ((auth_option_id = 14) OR (auth_role_id IN (1, 2, 4, 6, 10, 11, 12, 13, 14, 15, 17, 22, 23, 24))); 

