#!/usr/bin/gnuplot

#set size 1,1
#"NimbusSanL-Regu" fontfile "uhvr8a.pfb"
set terminal postscript colour eps enhanced "Helvetica" 25

set output 'results-temp.ps'
set border
set pointsize 1.3
set grid

#set terminal postscript enhanced color
#set output '| ps2pdf - plot.pdf'

set xlabel "GB Decrypted"
set ylabel "Time to decrypt (sec)"
plot   'fig/dec.txt'  using 1:2  title "" with linespoints lw 6
 
system "epstopdf results-temp.ps"
system "ps2pdf14 -dPDFSETTINGS=/prepress results-temp.pdf fig/dec.pdf"