#!/usr/bin/gnuplot

#set size 1,1
#"NimbusSanL-Regu" fontfile "uhvr8a.pfb"
set terminal postscript colour eps enhanced "Helvetica" 34

set output 'results-temp.ps'
set border
set pointsize 1.3
set grid

#set terminal postscript enhanced color
#set output '| ps2pdf - plot.pdf'

set xlabel "No. of Clients"
set ylabel "Transactions/sec" offset +2,0
set pointsize 4
set key bottom right
plot   'data.txt' using 1:3 title " Postgres" with linespoints lw 6, \
       'data.txt' using 1:4 title " CryptDB" with linespoints lw 6 lc rgb "black"
   
 
system "epstopdf results-temp.ps"
system "ps2pdf14 -dPDFSETTINGS=/prepress results-temp.pdf trantput1.pdf"
system "pdfcrop trantput1.pdf trantput.pdf"
system "rm results-temp.* trantput1.pdf"


